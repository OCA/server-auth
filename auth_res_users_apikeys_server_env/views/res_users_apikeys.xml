<odoo>
    <record model="ir.ui.view" id="res_users_apikeys_form_view">
        <field
            name="name"
        >res.users.apikeys.form (in auth_user_api_key_server_env)</field>
        <field name="model">res.users.apikeys</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <label for="name" class="oe_edit_only" />
                    <h1>
                        <field name="name" class="oe_inline" />
                    </h1>
                    <group name="config" colspan="4" col="4">
                        <field name="user_id" colspan="4" />
                        <field name="scope" colspan="4" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="res_users_apikeys_tree_view">
        <field
            name="name"
        >res.users.apikeys.tree (in auth_user_api_key_server_env)</field>
        <field name="model">res.users.apikeys</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="user_id" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="res_users_apikeys_act_window">
        <field name="name">Auth User API Key</field>
        <field name="res_model">res.users.apikeys</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
    <record model="ir.ui.menu" id="res_users_apikeys_menu">
        <field name="name">Auth User API Key</field>
        <field name="parent_id" ref="base.menu_custom" />
        <field name="action" ref="res_users_apikeys_act_window" />
        <field name="sequence" eval="100" />
    </record>
</odoo>
