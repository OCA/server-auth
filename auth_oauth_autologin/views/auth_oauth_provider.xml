<?xml version="1.0" ?>
<odoo>
    <record model="ir.ui.view" id="view_oauth_provider_form">
        <field name="name">auth.oauth.autologin.provider.form</field>
        <field name="model">auth.oauth.provider</field>
        <field name="inherit_id" ref="auth_oauth.view_oauth_provider_form" />
        <field name="arch" type="xml">
            <field name="enabled" position="after">
                <field name="autologin" />
            </field>
        </field>
    </record>
    <record model="ir.ui.view" id="view_oauth_provider_tree">
        <field name="name">auth.oauth.autologin.provider.form</field>
        <field name="model">auth.oauth.provider</field>
        <field name="inherit_id" ref="auth_oauth.view_oauth_provider_tree" />
        <field name="arch" type="xml">
            <field name="enabled" position="after">
                <field name="autologin" />
            </field>
        </field>
    </record>
</odoo>
